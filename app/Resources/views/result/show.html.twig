{% extends 'base.html.twig' %}

{% block body %}
    <h1>Result</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ result.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ result.name }}</td>
            </tr>
            <tr>
                <th>Created</th>
                <td>{% if result.created %}{{ result.created|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Updated</th>
                <td>{% if result.updated %}{{ result.updated|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Contentchangedby</th>
                <td>{{ result.contentChangedBy }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_result_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_result_edit', { 'id': result.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
